            "doc": "마을 임무 실행"
        }
    ],
    "option": {
        "서버선택": {
            "cases": [
                {
                    "name": "직영",
                    "pipeline_override": {}
                },
                {
                    "name": "비리비리",
                    "pipeline_override": {
                        "StartUp": {
                            "interrupt": "GS_startApp_bili"
                        }
                    }
                }
            ]
        }
