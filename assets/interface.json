{
    "name": "MFMA",
    "version":  "1.0.0",
    "mirrorchyan_rid":  null,
    "mirrorchyan_multiplatform":  false,
    "url":  "https://github.com/maynut02/MFMA",
    "custom_title": "MFMA - 신월동행 어시스턴트",
    "controller": [
        {
            "name": "安卓端",
            "type": "Adb"
        }
    ],
    "resource": [
        {
            "name": "중국서버",
            "path": [
                "{PROJECT_DIR}/resource"
            ]
        }
    ],
    "agent": {
        "child_exec": "python",
        "child_args": [
            "{PROJECT_DIR}/agent/main.py"
        ]
    },
    "task": [
        {
            "name": "로그인",
            "entry": "GameStart",
            "doc": "신월동행 접속",
            "option": [
                "서버선택"
            ],
            "check": true
        },
        {
            "name": "우편 수령",
            "entry": "VisitMail",
            "doc": "우편 수령"
        },
        {
            "name": "우정 교환",
            "entry": "VisitFriend",
            "doc": "우정 교환"
        },
        {
            "name": "상점 탐색",
            "entry": "VisitStore",
            "doc": "무료 구매 물품 자동 구매"
        },
        {
            "name": "스테미너 사용",
            "entry": "UseStamina",
            "doc": "스테이지 자동 반복으로 스테미너 사용\n- 회복약과 우유 회복(미완성)\n- 입장 가능한 최고 난이도 자동 지정(미완성)",
            "option": [
                "스테이지 선택"
            ]
        },
        {
            "name": "마을 임무(미완성)",
            "entry": "VisitTown",
            "doc": "마을 임무 실행"
        }
    ],
    "option": {
        "서버선택": {
            "cases": [
                {
                    "name": "비리비리",
                    "pipeline_override": {
                        "StartApp": {
                            "package": "com.firewick.p42.bilibili"
                        }
                    }
                },
                {
                    "name": "직영",
                    "pipeline_override": {
                        "StartApp": {
                            "package": "com.firewick.p42"
                        }
                    }
                }
            ]
        },
