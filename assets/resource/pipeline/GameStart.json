{
    "GameStart": {
        "next": [
            "GS_isHome",
            "GS_isPopup",
            "GS_terms",
            "GS_update",
            "GS_biliLogin",
            "GS_connect"
        ],
        "interrupt": [
            "GS_startApp"
        ],
        "timeout": 60000
    },

    "GS_startApp": {
        "action": "StartApp",
        "package": "com.firewick.p42",
        "post_delay": 3000
    },
    "GS_startApp_bili": {
        "action": "StartApp",
        "package": "com.firewick.p42.bilibili",
        "post_delay": 3000
    },

    "GS_terms": {
        "recognition": "OCR",
        "action": "Click",
        "expected": [
            "同意"
        ],
        "next": [
            "GameStart"
        ],
        "post_delay": 3000
    },

    "GS_update": {
        "recognition": "OCR",
        "action": "Click",
        "expected": [
            "新版本资源"
        ],
        "next": [
            "GS_updateConfirm"
        ],
        "post_delay": 500
    },
    "GS_updateConfirm": {
        "recognition": "OCR",
        "action": "Click",
        "expected": [
            "确定"
        ],
        "next": [
            "GameStart"
        ],
        "post_delay": 500
    },

    "GS_biliLogin": {
        "recognition": "OCR",
        "action": "Click",
        "expected": [
            "登录"
        ],
        "next": [
            "GameStart"
        ],
        "post_delay": 3000
    },

    "GS_connect": {
        "recognition": "OCR",
        "action": "Click",
        "expected": [
            "校准现实"
        ],
        "next": [
            "GameStart"
        ],
        "post_delay": 3000
    },

    "GS_isHome": {
        "recognition": "OCR",
        "action": "DoNothing",
        "expected": [
            "城市探索"
        ],
        "next": [
            "GS_isPopup"
        ],
        "on_error": [
            "GS_exit"
        ],
        "timeout": 5000
    },

    "GS_isPopup": {
        "recognition": "TemplateMatch",
        "action": "DoNothing",
        "template": [
            "../image/PopupClose.png"
        ],
        "next": [
            "GS_popupClose"
        ]
    },
    "GS_popupClose": {
        "recognition": "TemplateMatch",
        "action": "Click",
        "template": [
            "../image/PopupClose.png"
        ],
        "next": [
            "GS_isHome"
        ]
    },

    "GS_exit": {
        "recognition": "OCR",
        "action": "StopTask",
        "expected": [
            "城市探索"
        ]
    }
}